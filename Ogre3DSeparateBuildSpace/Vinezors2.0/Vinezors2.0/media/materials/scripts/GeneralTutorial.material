material General/TutorialBackdrop
{
    technique
    {
      pass
      {
        lighting off
        scene_blend alpha_blend
        depth_write off
        texture_unit
        {
            texture back_drop.png
            tex_border_colour 0 0 0 0
            tex_address_mode border
        }
      } 
    }
}

